{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "strict": true,
    "target": "ES2020",
    "module": "ES2020",
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "declaration": true,
    "moduleResolution": "node"
  },
}
