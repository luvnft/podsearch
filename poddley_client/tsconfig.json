{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "noImplicitAny": true,
    "strictNullChecks": true,
    "alwaysStrict": true,
    "strict": true,
    "target": "es6",
    "module": "commonjs",
    "strictFunctionTypes": true,
    "strictPropertyInitialization": true,
    "noImplicitThis": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "outDir": "dist",
    "declaration": true,
    "declarationDir": "dist/types",
  }
}
