  "scripts": {
    "production:buildClient": "cd client && nuxt build --modern=true",
    "production:runClient": "cd client && node ./.output/server/index.mjs",
    "production:buildBackend": "cd backend && tsc",
    "production:runBackend": "fuser -k 3000/tcp || true && cd backend && node ./dist/app.js",
    "migrate": "npx prisma migrate dev --schema=./backend/prisma/schema.prisma",
    "dev": "cd client && nuxt dev --modern=server",
    "generateprismaclient": "npx prisma generate --schema=./backend/prisma/schema.prisma",
    "deleteeverythinglol": "dotenv -- sh -c 'cd backend/other && ts-node database_drop_all_tables.ts && ts-node meilisearch_purge_everything_script.ts && npm run migrate'"
